@model List<Quiz>

@{ 
    int e = 1;
}

<table class="table table-bordered table-condensed table-hover table-striped">
    <tr>
        <th>Id</th>
        <th>Title</th>
        <th>Topic</th>
        <th>Exam Level</th>
        <th>Exam Date</th>
        <th>Add To Quiz?</th>
    </tr>


    @foreach (var item in Model)

    {
        <tr>
            <td>
                @e
            </td>
            <td>
                @item.Title
            </td>
            <td>
                @item.Topic
            </td>
            <td>
                @item.Sec
            </td>
            <td>
                @item.StartDt
            </td>
            <td>
                <input type="radio" id="rdOption1" value="@item.QuizId" name="@("Add" +e)" /> Yes
                <input type="radio" id="rdOption2" value="-1" name="@("Add" +e)" /> No
            </td>
        </tr>
        e++;
    }

</table>